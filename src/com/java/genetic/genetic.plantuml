title __GENETIC's Class Diagram__\n

  package  {
    package com.java.genetic {
      abstract class Brin {
          - strand : ArrayList<Nucleotide>
          + toString()
          + getStrand()
      }
    }
  }
  

  package  {
    package com.java.genetic {
      class Codon {
